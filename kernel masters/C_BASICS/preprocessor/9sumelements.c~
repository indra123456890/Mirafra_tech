/*9 Using the above macro, write a program to compute the sum of all elements in an array.*/

#include<stdio.h>
#define s(a,n) for(i=0;i<n;i++)\
{\
	sum=sum+a[i];\
}\
{
	int main()
{
	int i,sum=0,n;
	printf("enter the array size");
	scanf("%d",&n);
	int a[n];
	printf("enter the elements");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	s(a,n)
		printf("%d\n",sum);
}
}

